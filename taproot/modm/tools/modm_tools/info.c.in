#include "info_{{ type }}.h"

{% for name, value in defines.items() | sort %}
{%- if value is number %}
const {{ "uint32_t" if "TIMESTAMP" in name else "int16_t" }} {{ name | upper }} = {{ value }};
{%- else %}
const char *{{ name | upper }} = "{{ value | replace('"', '\\"') }}";
{%- endif %}
{%- endfor %}
