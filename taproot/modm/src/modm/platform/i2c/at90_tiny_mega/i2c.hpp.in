#ifndef MODM_AVR_I2C_HPP
#define MODM_AVR_I2C_HPP

#include <avr/io.h>

namespace modm
{

namespace platform
{

/// @ingroup	modm_platform_i2c
struct I2c
{
	enum class
	Prescaler : uint8_t
	{
		Div1  = 0,
		Div4  = (1 << TWPS0),
		Div16 = (1 << TWPS1),
		Div64 = (1 << TWPS1) | (1 << TWPS0)
	};
};

} // namespace platform

} // namespace modm

#endif	// MODM_AVR_I2C_HPP
